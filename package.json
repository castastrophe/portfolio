{
  "name": "personal-site",
  "version": "2.0.0",
  "description": "Personal portfolio site",
  "keywords": [
    "personal site"
  ],
  "main": "index.js",
  "homepage": "https://allons-y.llc",
  "browserslist": [
    "last 2 versions",
    "cover 99.5% in US",
    "not ie 11",
    "not dead"
  ],
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-navigation": "^0.3.5",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@csstools/postcss-sass": "^5.0.1",
    "@shoelace-style/shoelace": "^2.0.0-beta.83",
    "autoprefixer": "^10.4.13",
    "eleventy-plugin-postcss": "^1.0.4",
    "eleventy-plugin-youtube-embed": "^1.7.1",
    "html-minifier": "^4.0.0",
    "luxon": "^3.1.0",
    "markdown-it": "^13.0.1",
    "markdown-it-anchor": "^8.6.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "10.0.0",
    "postcss-plugin": "^1.0.0",
    "postcss-preset-env": "^7.8.3",
    "postcss-scss": "^4.0.5"
  },
  "scripts": {
    "clean": "rm -rf _site",
    "prebuild": "yarn clean",
    "build": "run-p build:sass build:site",
    "build:site": "npx @11ty/eleventy",
    "build:sass": "[ ! -d '_site' ] && mkdir _site; yarn postcss ./content/css/index.scss --config postcss.config.js --dir ./_site/css/ --ext css --watch=${WATCH_MODE:=false}",
    "bench": "DEBUG=Eleventy:Benchmark* yarn build",
    "serve": "yarn build --serve",
    "start": "WATCH_MODE=true run-p build:sass 'build:site --serve'",
    "debug": "DEBUG=* npx @11ty/eleventy",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/castastrophe/portfolio.git"
  },
  "author": "Cassondra Roberts <castastrophe@users.noreply.github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/castastrophe/portfolio/issues"
  },
  "private": true
}
