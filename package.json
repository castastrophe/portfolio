    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.1",
    "@shoelace-style/shoelace": "^2.0.0",
    "autoprefixer": "^10.4.13",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "esmify": "^2.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
